{
  "manifest_version": 2,
  "name": "Memory Widget",
  "version": "0.1.0",
  "description": "Monitor Chrome's memory usage.",
  "permissions": [
    "processes",
    "tabs"
  ],
  "browser_action": {
      "default_title": "Memory Widget"
  },
  "background": {
    "page" : "background.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
